{
  "config": {
    "token": "",
    "prefixes": ["{{when_mentioned}}", ">"],
    "case_sensitive": false,
    "owner": 556119013298667520,
    "presence": {
      "type": "listening",
      "text": "discord.json <3"
    }
  },
  "events": [
    {
      "name": "on_ready",
      "action": {
        "type": "log",
        "value": "Bot is ready"
      }
    }
  ],
  "commands": [
    {
      "name": "ping",
      "reply": "Pong! {{bot.latency}}ms"
    },
    {
      "name": "hi",
      "aliases": ["hello", "info"],
      "response": "Hi, {{author.mention}} I am a bot that was made in discord.json (powered by discord-lib)"
    },
    {
      "name": "test",
      "action": {
        "type": "store",
        "name": "result",
        "data": {
          "type": "conditional",
          "condition": "1>2",
          "true": "It was true",
          "false": "It was false"
        }
      },
      "response": "Test {{result}}"
    }
  ]
}
