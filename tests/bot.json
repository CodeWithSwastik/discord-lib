{
  "config": {
    "token": "",
    "prefixes": ["{{when_mentioned}}", ">"],
    "case_sensitive": false,
    "owner": 556119013298667520,
    "presence": {
      "type": "listening",
      "text": "discord.json <3"
    },
    "allowed_mentions": ["users", "replied_user"]
  },
  "events": [
    {
      "name": "on_ready",
      "action": {
        "type": "log",
        "value": "Bot is ready"
      }
    }
  ],
  "commands": [
    {
      "name": "ping",
      "action": {
        "type": "store",
        "name": "latency",
        "data": {
          "type": "math",
          "value": "round({{bot.latency}} * 1000)"
        }
      },
      "reply": "Pong :ping_pong: {{latency}}ms"
    },
    {
      "name": "hi",
      "aliases": ["hello", "info"],
      "response": "Hi, {{author.mention}} I am a bot that was made in discord.json (powered by discord-lib)"
    },
    {
      "name": "repeat",
      "args": ["*lol"],
      "response": "{{lol}}"
    },
    {
      "name": "test",
      "args": ["lol"],
      "action": {
        "type": "math",
        "value": "{{lol}}"
      }
    }
  ]
}
